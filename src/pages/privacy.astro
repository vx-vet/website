---
import BaseLayout from "../layouts/BaseLayout.astro";
import Navigation from "../components/global/Navigation.astro";
import PrivacyComponent from "../components/infopages/Privacy.astro";
import Footer from "../components/global/Footer.astro";

import { AstroSeo } from "@astrolib/seo";
---

<!---
Use AstroSeo in all the pages you want different Seo than the index page
-->
<AstroSeo
  title="VX - Privacy"
  description="นโยบายความเป็นส่วนตัว"
  canonical="https://vx.vet/privacy"
  openGraph={{
    url: "https://vx.vet/privacy",
    title: "Privacy Policy",
    description: "นโยบายความเป็นส่วนตัว",
    images: [
      {
        url: "https://vx.vet/meta_og_image.jpg",
        width: 1200,
        height: 628,
        alt: "VX",
        type: "image/jpeg",
      },
    ],
    site_name: "VX",
  }}
/>
<BaseLayout>
  <Navigation />
  <PrivacyComponent />
  <Footer />
</BaseLayout>
