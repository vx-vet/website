---
import BaseLayout from "../layouts/BaseLayout.astro";
import Navigation from "../components/global/Navigation.astro";

import Hero from "../components/labx/Hero.astro";
import SectionOne from "../components/labx/SectionOne.astro";
import Team from "../components/landing/Team.astro";
import Footer from "../components/global/Footer.astro";
import { AstroSeo } from "@astrolib/seo";
---

<!---
Use AstroSeo in all the pages you want different Seo than the index page
-->
<AstroSeo
  title="LabX - จัดการผลแล็บ แบบอัจฉริยะ"
  description="Revolutionizes the future of veterinary business."
  canonical="https://vx.vet"
  openGraph={{
    url: "https://vx.vet",
    title: "LabX - จัดการผลแล็บ แบบอัจฉริยะ",
    description:
      "แอพพลิเคชั่นจัดการผลแล็บของสัตว์แพทย์ นำเข้าผลแล็บได้ทุกแบบ แสดงผลดูง่าย ค้นหาข้อมูลสะดวก",
    images: [
      {
        url: "https://vx.vet/meta_og_image.jpg",
        width: 1200,
        height: 628,
        alt: "VX",
        type: "image/jpeg",
      },
    ],
    site_name: "VX",
  }}
/>
<BaseLayout>
  <Navigation />
  <Hero />

  <SectionOne />
  <div class="mb-20">
    <Team />
  </div>
  <Footer />
</BaseLayout>
